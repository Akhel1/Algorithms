def isprime(num):
    c=0
    for i in range(1,num+1):
        if(num%i==0):
            c+=1
    if c==2:
        return True
    elif c>2:
        return False
def LarPrimeFact(n):
    LargestPrimeFactor = 0
    for i in range(1,n+1):
        if(n%i==0):
            if isprime(i):
                LargestPrimeFactor = i
    return LargestPrimeFactor

if __name__ == '__main__':
    n=13195
    LargestPrimeFactor=LarPrimeFact(n)
    print(LargestPrimeFactor)
